{% load static %}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://fonts.googleapis.com/css?family=Francois+One" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat|Raleway" rel="stylesheet">
     <link rel="stylesheet" href="{% static "msn/css/main.css" %}" />
    {% block head %}
    <title> {% block title %}{% endblock %} : Turnex </title>
    {% endblock %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
</head>
<body id="body">
<div class ="wrapper" id="wrapper">
    {% block header %}{% endblock %}
    <div class="container">
        {% block content %}{% endblock %}
        {% block sidebar%}{% endblock %}
    </div>
    {% block footer %}{% endblock %}
     {% block scripts %}{% endblock %}
     <input type="hidden" value="{{ debug }}" id="debug" name ="debug" />
     </div>
</body>
</html>
<!-- If you will use a new block, make sure you add a <div class="<my new block>...</div>" within the block -->