{% load static %}

<!DOCTYPE html>
<html lang="en">
<head>
    <link href="https://fonts.googleapis.com/css?family=Francois+One" rel="stylesheet">
     <link rel="stylesheet" href="{% static "msn/css/main.css" %}" />
    {% block head %}
    <title> {% block title %}{% endblock %} : Turnex </title>
    {% endblock %}
</head>
<body>
<div class ="wrapper">
    {% block header %}{% endblock %}
    <div class="container">
        {% block content %}{% endblock %}
        {% block sidebar%}{% endblock %}
    </div>
    {% block footer %}{% endblock %}
     {% block scripts %}{% endblock %}
     </main>
</body>
</html>
<!-- If you will use a new block, make sure you add a <div class="<my new block>...</div>" within the block -->